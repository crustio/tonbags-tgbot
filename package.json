{
  "name": "TG-bags-bot",
  "version": "1.0.0",
  "scripts": {
    "dev": "node --require ts-node/register src/main.ts",
    "compile": "npx rimraf dist && tsc && cp -R src/sql dist && cp src/crust.jpg dist",
    "run": "node ./dist/main.js",
    "start:redis": "docker run -d -p 127.0.0.1:6379:6379 redis/redis-stack-server:latest",
    "start:daemon": "pm2 start --name tgbot ./dist/main.js --watch --no-daemon",
    "stop:daemon": "pm2 stop tgbot && pm2 delete tgbot"
  },
  "dependencies": {
    "@ton/core": "^0.56.3",
    "@ton/crypto": "^3.3.0",
    "@ton/ton": "^14.0.0",
    "@tonconnect/sdk": "^3.0.0-beta.1",
    "aws-sdk": "^2.1664.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "form-data": "^4.0.1",
    "js-sha256": "^0.11.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.3",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.66.0",
    "postgrator": "4.3.1",
    "qrcode": "^1.5.1",
    "redis": "^4.6.15",
    "sequelize": "^6.37.4",
    "uuid": "^10.0.0",
    "viem": "^2.21.31",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.12",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/qrcode": "^1.5.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "pm2": "^5.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "5.x"
  }
}
